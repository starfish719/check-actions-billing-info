{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@octokit/core": "^3.2.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.31",
    "@typescript-eslint/parser": "^5.19.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.20.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.5",
    "git-cz": "^4.7.6",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.5"
  }
}
